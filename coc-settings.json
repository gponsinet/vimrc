{
  "json.enable": true,
  "json.format.enable": true,

  "html.enable": true,
  "html.filetypes": ["html", "handlebars"],
  "html.format": true,
  "html.validate.script": true,
  "html.validate.styles": true,

  "hightlight.disableLanguages": [],
  "hightlight.document.enable": true,
  "highlight.colors.enable": true,

  "snippets.priority": 90,
  //  "snippets.userSnippetsDirectory":
  "snippets.shortcut": "S",
  "snippets.autoTrigger": true,
  "snippets.triggerCharacters": [],
  "snippets.loadFromExtensions": true,
  // "snippets.textmateSnippetsRoots": ["./snippets/vscode", "./snippets/textmate"],
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.usePythonx": true,
  "snippets.ultisnips.pythonVersion": 3,
  // "snippets.ultisnips.directories": ["./snippets/ultisnips"],
  "snippets.snipmate.enable": true,
  // "snippets.snippets.author":

  "tsserver.enable": true,
  "tsserver.locale": "",
  "tsserver.formatOnType": true,
  "tsserver.enableJavascript": true,
  // "tsserver.implicitProjectConfig.experimentalDecorators": true,

  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "eslint.run": "onType",
  "eslint.codeAction.disableRuleComment": {
    "enable": true,
    "location": "sameLine"
  },
  "eslint.codeAction.showDocumentation": true,

  "go.enable": true,
  "go.goplsOptions": {
    "completeUnimported": true,
    "completionDocumentation": true,
    "deepCompletion": true,
    "staticcheck": true,
    "usePlaceholders": true
  },

  // Default config

  // could be 'always' 'trigger' => for specified trigger characters only 'none'
  "coc.preferences.autoTrigger": "always",
  // only used when autoTrigger is always
  "coc.preferences.triggerAfterInsertEnter": false,
  // timeout for completion
  "coc.preferences.timeout": 500,
  // make vim select first item on completion
  "coc.preferences.noselect": true,
  // when snippet activated and pumvisible, prefer complete completion.
  "coc.preferences.preferCompleteThanJumpPlaceholder": true,
  // enable formatOnType feature
  "coc.preferences.formatOnType": false,
  // command used for jump to other buffer
  "coc.preferences.jumpCommand": "edit",
  // executable path for https://facebook.github.io/watchman/, detected from $PATH by default
  // "coc.preferences.watchmanPath": "",
  // enable diagnostic
  "coc.preferences.diagnostic.enable": true,
  "coc.preferences.diagnostic.signOffset": 1000,
  "coc.preferences.diagnostic.errorSign": ">>",
  "coc.preferences.diagnostic.warningSign": "âš ",
  "coc.preferences.diagnostic.infoSign": ">>",
  "coc.preferences.diagnostic.hintSign": ">>",
  // enable coc provide color highlight
  "coc.preferences.colorSupport": true,


  // Language server protocol
  "languageserver": {
    // "golang": {
    //   "command": "gopls",
    //   "rootPatterns": ["go.mod"],
    //   "filetypes": ["go"]
    // },
    // "typescript": {
    //   "command": "javascript-typescript-stdio",
    //   "filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact"]
    // },
    // "js-languageserver": {
    //   "command": "js-langserver",
    //   "args": ["--stdio"],
    //   "filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact"]
    // }
  }

}
